local UIS = game:GetService("UserInputService")

UIS.InputBegan:Connect(function(input)
	if input.KeyCode == Enum.KeyCode.Delete then
		game.CoreGui:FindFirstChild("ItemGiverGui"):Destroy()
	end
end)

